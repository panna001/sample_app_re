<div class="list-wrap">
    <%= @lists.each do |list| %>
        <div class="list-box">
            <%= attachmetnt_image_tag list, :fill, 200, 200, format: "jpeg" %>
            <div class="list-cometnt">
                <span class="list-body"><%= list.body %></span>
                <span class="list-title"><%= link_to list.title, todolists_path(list.id) %></span>
            </div>
        </div>
    <% end %>
</div>
